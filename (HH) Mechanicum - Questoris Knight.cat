    <selectionEntry id="33d55e37-58db-535e-ffb4-0c9136c15e25" name="Seneschal" book="HH4: Conquest" page="295" hidden="false" collective="false" type="unit">
      <profiles/>
      <rules>
        <rule id="22371748-eb17-a4bd-f77d-29db844784f2" name="Master Knight" book="HH4: Conquest" page="295" hidden="false">
          <profiles/>
          <rules/>
          <infoLinks/>
          <modifiers/>
          <description>Add +1 to the Weapon Skill and Ballistic Skill shown on the Knight&apos;s profile, and increase any invulnerable save by +1.</description>
        </rule>
        <rule id="7efce086-cd0a-a073-5cae-d76620d25977" name="Ideal Mission Commander" book="HH4: Conquest" page="295" hidden="false">
